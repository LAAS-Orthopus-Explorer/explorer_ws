<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:macro name="explorer_old_description">

  <!-- helper macro -->
  <xacro:include filename="$(find ros2_control_explorer)/description/urdf/inc/create_link.xacro"/>    

  <xacro:create_link
      link_name="base_link"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="ros2_control_explorer"
      mesh_name="base_link"
      xyz_mass="0.000045 0.000001 0.026873"
      mass="0.520"
      ixx="0.000369892299"
      ixy="2.16179e-7"
      ixz="2.63279e-7"
      iyy="0.000366673241"
      iyz="1.3226e-8"
      izz="0.000433449993"
  />

  <xacro:create_link
      link_name="link_1"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="ros2_control_explorer"
      mesh_name="link1"
      xyz_mass="-0.000001 0.02935 0.029854"
      mass="0.318"
      ixx="0.000323959935"
      ixy="3.991e-9"
      ixz="9.773e-9"
      iyy="0.000322143455"
      iyz="-0.000086000965"
      izz="0.000237019412"
  />

  <xacro:create_link
      link_name="link_2"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="ros2_control_explorer"
      mesh_name="link2"
      xyz_mass="0.204774 0.000012 0.057505"
      mass="2.023"
      ixx="0.002433042798"
      ixy="1.94199e-7"
      ixz="0.000001567055"
      iyy="0.057628887234"
      iyz="5.66556e-7"
      izz="0.000028172073"
  />

  <xacro:create_link
      link_name="link_3"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="ros2_control_explorer"
      mesh_name="link3"
      xyz_mass="0.138064 -0.000019  -0.007584"
      mass="1.074"
      ixx="0.000563205607"
      ixy="0.00000129196"
      ixz="0.000588051397"
      iyy="0.006043403387"
      iyz="1.69302e-7"
      izz="0.006205261854"
  />

  <xacro:create_link
      link_name="link_4"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="ros2_control_explorer"
      mesh_name="link4_old"
      xyz_mass="0.000006 0.044695  0.057883"
      mass="0.687"
      ixx="0.001204142247"
      ixy="-0.000001088536"
      ixz="-4.28569e-7"
      iyy="0.000794422875"
      iyz="-0.000320135516"
      izz="0.00092964379"
  />

  <xacro:create_link
      link_name="link_5"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="ros2_control_explorer"
      mesh_name="link5_old"
      xyz_mass="0.000006 0.044695  0.057883"
      mass="0.687"
      ixx="0.001204142247"
      ixy="-0.000001088536"
      ixz="-4.28569e-7"
      iyy="0.000794422875"
      iyz="-0.000320135516"
      izz="0.00092964379"
  />

  <xacro:create_link
      link_name="link_6"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="ros2_control_explorer"
      mesh_name="gripper"
      xyz_mass="0.00224 -0.000032 0.077277"
      mass="0.407"
      ixx="0.001506746748"
      ixy="9.31438e-7"
      ixz="-0.000095753657"
      iyy="0.001072546483"
      iyz="2.73718e-7"
      izz="0.000713799995"
  />

  <link name="ft_frame"/>
  <link name="tool0"/>

  <!-- base_joint fixes base_link to the environment -->
  <joint name="base_joint" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <!-- joints - main serial chain -->
  <joint name="joint_1" type="revolute">
    <parent link="base_link"/>
    <child link="link_1"/>
    <origin xyz="0 0 0.0555" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="${-pi}" upper="${pi}" velocity="2.5"/>
  </joint>

  <joint name="joint_2" type="revolute">
    <parent link="link_1"/>
    <child link="link_2"/>
    <origin xyz="0 0.0245 0.058952" rpy="${pi/2} ${-pi/2} 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="${-pi/2}" upper="${pi/2}" velocity="2.5"/>
  </joint>

  <joint name="joint_3" type="revolute">
    <parent link="link_2"/>
    <child link="link_3"/>
    <origin xyz="0.409872 0 -0.002" rpy="${pi} 0 ${-pi/2}"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="${-3*pi/2}" upper="${pi/2}" velocity="2.5"/>
  </joint>

  <joint name="joint_4" type="revolute">
    <parent link="link_3"/>
    <child link="link_4"/>
    <origin xyz="0.2339 0 -0.014" rpy="0 ${pi/2} 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="${-pi}" upper="${pi}" velocity="2.5"/>
  </joint>

  <joint name="joint_5" type="revolute">
    <parent link="link_4"/>
    <child link="link_5"/>
    <origin xyz="0 0.090 0.083" rpy="${(65*pi)/180} 0 ${pi}"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="${-pi}" upper="${pi}" velocity="2.5"/>
  </joint>

  <joint name="joint_6" type="revolute">
    <parent link="link_5"/>
    <child link="link_6"/>
    <origin xyz="0 0.090 0.083" rpy="${-(65*pi)/180} 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="${-pi}" upper="${pi}" velocity="2.5"/>
  </joint>

  <joint name="joint-ft_frame" type="fixed">
    <parent link="link_6"/>
    <child link="ft_frame"/>
    <origin xyz="0.011 0 0.15" rpy="0 0 0"/>
  </joint>

  <joint name="joint-tool" type="fixed">
    <parent link="ft_frame"/>
    <child link="tool0"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

</xacro:macro>

</robot>