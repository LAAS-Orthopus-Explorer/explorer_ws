<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:macro name="explorer_old_description">

  <!-- helper macro -->
  <xacro:include filename="$(find ros2_control_explorer)/description/urdf/inc/create_link.xacro"/>    

  <xacro:create_link
      link_name="base_link"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="ros2_control_explorer"
      mesh_name="base_link"
      xyz_mass="0.000045 0.000001 0.026873"
      mass="0.520"
      ixx="369892.299"
      ixy="-216.179"
      ixz="263.279"
      iyy="366673.241"
      iyz="13.226"
      izz="433449.993"
  />

  <xacro:create_link
      link_name="link_1"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="ros2_control_explorer"
      mesh_name="link1"
      xyz_mass="-0.000001 0.02935 0.029854"
      mass="0.318"
      ixx="323959.935"
      ixy="3.991"
      ixz="9.773"
      iyy="322143.455"
      iyz="-86000.965"
      izz="237019.412"
  />

  <xacro:create_link
      link_name="link_2"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="ros2_control_explorer"
      mesh_name="link2"
      xyz_mass="0.204774 0.000012 0.057505"
      mass="2.023"
      ixx="2433042.798"
      ixy="194.199"
      ixz="1567.055"
      iyy="57628887.234"
      iyz="566.556"
      izz="28172.073"
  />

  <xacro:create_link
      link_name="link_3"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="ros2_control_explorer"
      mesh_name="link3"
      xyz_mass="0.138064 -0.000019  -0.007584"
      mass="1.074"
      ixx="563205.607"
      ixy="1291.964"
      ixz="588051.397"
      iyy="6043403.387"
      iyz="169.302"
      izz="6205261.854"
  />

  <xacro:create_link
      link_name="link_4"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="ros2_control_explorer"
      mesh_name="link4_old"
      xyz_mass="0.000006 0.044695  0.057883"
      mass="0.687"
      ixx="1204142.247"
      ixy="-1088.536"
      ixz="-428.569"
      iyy="794422.875"
      iyz="-320135.516"
      izz="929643.790"
  />

  <xacro:create_link
      link_name="link_5"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="ros2_control_explorer"
      mesh_name="link5_old"
      xyz_mass="0.000006 0.044695  0.057883"
      mass="0.687"
      ixx="1204142.247"
      ixy="-1088.536"
      ixz="-428.569"
      iyy="794422.875"
      iyz="-320135.516"
      izz="929643.790"
  />

  <xacro:create_link
      link_name="link_6"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="ros2_control_explorer"
      mesh_name="gripper"
      xyz_mass="0.00224 -0.000032 0.077277"
      mass="0.407"
      ixx="1506746.748"
      ixy="931.438"
      ixz="-95753.657"
      iyy="1072546.483"
      iyz="273.718"
      izz="713799.995"
  />

  <link name="ft_frame"/>
  <link name="tool0"/>

  <!-- base_joint fixes base_link to the environment -->
  <joint name="base_joint" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <!-- joints - main serial chain -->
  <joint name="joint_1" type="revolute">
    <parent link="base_link"/>
    <child link="link_1"/>
    <origin xyz="0 0 0.0555" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="${-pi}" upper="${pi}" velocity="2.5"/>
  </joint>

  <joint name="joint_2" type="revolute">
    <parent link="link_1"/>
    <child link="link_2"/>
    <origin xyz="0 0.0245 0.058952" rpy="${pi/2} ${-pi/2} 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="${-pi/2}" upper="${pi/2}" velocity="2.5"/>
  </joint>

  <joint name="joint_3" type="revolute">
    <parent link="link_2"/>
    <child link="link_3"/>
    <origin xyz="0.409872 0 -0.002" rpy="${pi} 0 ${-pi/2}"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="${-3*pi/2}" upper="${pi/2}" velocity="2.5"/>
  </joint>

  <joint name="joint_4" type="revolute">
    <parent link="link_3"/>
    <child link="link_4"/>
    <origin xyz="0.2339 0 -0.014" rpy="0 ${pi/2} 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="${-pi}" upper="${pi}" velocity="2.5"/>
  </joint>

  <joint name="joint_5" type="revolute">
    <parent link="link_4"/>
    <child link="link_5"/>
    <origin xyz="0 0.090 0.083" rpy="${(65*pi)/180} 0 ${pi}"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="${-pi}" upper="${pi}" velocity="2.5"/>
  </joint>

  <joint name="joint_6" type="revolute">
    <parent link="link_5"/>
    <child link="link_6"/>
    <origin xyz="0 0.090 0.083" rpy="${-(65*pi)/180} 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="${-pi}" upper="${pi}" velocity="2.5"/>
  </joint>

  <joint name="joint-ft_frame" type="fixed">
    <parent link="link_6"/>
    <child link="ft_frame"/>
    <origin xyz="0.011 0 0.15" rpy="0 0 0"/>
  </joint>

  <joint name="joint-tool" type="fixed">
    <parent link="ft_frame"/>
    <child link="tool0"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

</xacro:macro>

</robot>